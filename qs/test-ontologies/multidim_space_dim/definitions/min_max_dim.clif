

(cl-text http://colore.oor.net/multidim_space_dim/definitions/min_max_dim.clif



(cl-imports http://colore.oor.net/multidim_space_dim/dim_prime_linear_unbounded.clif)

(cl-imports http://colore.oor.net/multidim_space_dim/definitions/dim_basic_defs.clif)




(forall (x)
	(iff	
        (MaxDim x)
        (and
            (S x)
            (not (exists (y) 
                (and
                    (S y)
                    (< x y)
                )
            ))
        )
	)
)


(forall (x)
	(iff	
		(MinDim x)
		(and
            (S x)
			(not (ZEX x))
			(forall (y)
				(if
					(and
                        (S y)
                        (< y x)
                    )
					(ZEX y)
				)
			)	
		)
	)
)


)



